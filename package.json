{
	"name": "ionic-logging-viewer-app",
	"version": "0.0.0-development",
	"description": "Test app for ionic-logging-viewer",
	"author": {
		"name": "Markus Wagner",
		"email": "markus.wagner02@web.de"
	},
	"repository": "https://github.com/Ritzlgrmft/ionic-logging-viewer.git",
	"license": "MIT",
	"private": false,
	"keywords": [
		"ionic",
		"mobile",
		"logging",
		"log4javascript"
	],
	"scripts": {
		"build": "ng build ionic-logging-viewer --prod && cpx '{BREAKING,CHANGELOG,README}.md' dist/ionic-logging-viewer --verbose",
		"test": "ng test ionic-logging-viewer --code-coverage --watch false --browsers ChromeHeadless && compodoc --coverageTest 100 && ng lint && ng build ionic-logging-viewer --prod && ng e2e",
		"test-watch": "ng test ionic-logging-viewer --code-coverage",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"codecov": "codecov",
		"compodoc": "compodoc",
		"semantic-release": "semantic-release --dry-run",
		"travis-deploy-once": "travis-deploy-once"
	},
	"dependencies": {
		"@angular/animations": "^6.1.0",
		"@angular/common": "^6.1.0",
		"@angular/compiler": "^6.1.0",
		"@angular/core": "^6.1.0",
		"@angular/forms": "^6.1.0",
		"@angular/http": "^6.1.0",
		"@angular/platform-browser": "^6.1.0",
		"@angular/platform-browser-dynamic": "^6.1.0",
		"@angular/router": "^6.1.0",
		"@compodoc/compodoc": "1.1.5",
		"@ionic/angular": "^4.0.0-beta.13",
		"core-js": "^2.5.4",
		"ionic-logging-service": "^6.0.0",
		"rxjs": "~6.2.0",
		"zone.js": "~0.8.26"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.8.0",
		"@angular-devkit/build-ng-packagr": "~0.8.0",
		"@angular/cli": "~6.2.5",
		"@angular/compiler-cli": "^6.1.0",
		"@angular/language-service": "^6.1.0",
		"@types/jasmine": "2.8.9",
		"@types/jasminewd2": "2.0.5",
		"@types/node": "~8.9.4",
		"codecov": "3.1.0",
		"codelyzer": "4.5.0",
		"jasmine-core": "~2.99.1",
		"jasmine-spec-reporter": "4.2.1",
		"karma": "~3.0.0",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "2.0.4",
		"karma-jasmine": "1.1.2",
		"karma-jasmine-html-reporter": "^0.2.2",
		"ng-packagr": "^4.1.0",
		"protractor": "~5.4.0",
		"semantic-release": "15.10.5",
		"travis-deploy-once": "5.0.9",
		"ts-node": "~7.0.0",
		"tsickle": ">=0.29.0",
		"tslib": "^1.9.0",
		"tslint": "~5.11.0",
		"typescript": "~2.9.2"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"release": {
		"pkgRoot": "dist/ionic-logging-viewer"
	}
}
