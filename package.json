{
  "name": "ionic-logging-viewer",
  "version": "0.0.0-development",
  "description": "Viewer component for logs written by ionic-logging-service",
  "author": {
    "name": "Markus Wagner",
    "email": "markus.wagner02@web.de"
  },
  "repository": "https://github.com/Ritzlgrmft/ionic-logging-viewer.git",
  "license": "MIT",
  "private": false,
  "keywords": [
    "ionic",
    "mobile",
    "logging",
    "log4javascript"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile && npm run copy-scss",
    "clean": "rm -f dist/*.d.ts && rm -f dist/*.js && rm -f dist/*.map && rm -f dist/*.json && rm -f dist/*.scss && rm -f src/*.ngfactory.ts",
    "commit": "git-cz",
    "compile": "tsc -p tsconfig.json && rollup -c rollup.config.js dist/logging-viewer.module.js > dist/logging-viewer.bundle.js && ngc -p tsconfig.aot.json",
    "copy-scss": "cp src/*.scss dist",
    "karma": "karma start karma.conf.js --browsers Chrome",
    "karma-once": "karma start karma.conf.js --browsers PhantomJS --single-run",
    "lint": "tslint --project tsconfig.aot.json --format verbose",
    "pack": "npm run build && npm pack",
    "report-coverage": "codecov",
    "semantic-release": "semantic-release",
    "test": "npm run build && npm run lint",
    "travis-deploy-once": "travis-deploy-once"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/logging-viewer.bundle.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "peerDependencies": {
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/platform-server": "^5.0.0",
    "ionic-angular": "^3.9.0",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.18"
  },
  "dependencies": {
    "ionic-logging-service": "^5.0.0"
  },
  "devDependencies": {
    "@angular/animations": "5.0.0",
    "@angular/common": "5.0.0",
    "@angular/compiler": "5.0.0",
    "@angular/compiler-cli": "5.0.0",
    "@angular/core": "5.0.0",
    "@angular/forms": "5.0.0",
    "@angular/http": "5.0.0",
    "@angular/platform-browser": "5.0.0",
    "@angular/platform-browser-dynamic": "5.0.0",
    "@angular/platform-server": "5.0.0",
    "ionic-angular": "3.9.2",
    "rxjs": "5.5.2",
    "zone.js": "0.8.18",
    "@types/jasmine": "2.8.7",
    "babel-polyfill": "6.26.0",
    "codecov": "3.0.2",
    "codelyzer": "4.3.0",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "jasmine-core": "2.99.1",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.2",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-typescript": "3.0.12",
    "karma-typescript-es6-transform": "1.0.4",
    "reflect-metadata": "0.1.12",
    "requirejs": "2.3.5",
    "rollup": "0.56.3",
    "tslint": "5.10.0",
    "typescript": "2.4.2",
    "semantic-release": "15.4.1",
    "travis-deploy-once": "5.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
